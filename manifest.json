{
    "manifest_version": 3,
    "name": "AWS Bookmaker Normaliser",
    "description": "Normalises AWS Bookmaker names to their standard acronyms",
    "version": "1.0.0",
    "author": "gday@danferg.com",
    "permissions": [
        "activeTab"
    ],
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.console.aws.amazon.com/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}